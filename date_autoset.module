<?php
/**
 * Implementation of hook_elements().
 */
function date_autoset_elements() {
  return array(
    'date_popup' => array(
      '#input' => TRUE,
      '#tree' => TRUE,
      '#process' => array('date_autoset_process')
    ),
    'date' => array(
      '#input' => TRUE,
      '#tree' => TRUE,
      '#process' => array('date_autoset_process')
    )
  );
}

function date_autoset_process($element, $edit, $form_state, $form) {
  if ($element["#title"] == "From date") {
    drupal_add_js(drupal_get_path('module', 'date_autoset') . "/date_autoset.js");

    $date1 = '#' . $element["date"]["#id"];
    $date2 = str_replace("value", "value2", $date1);
    $time1 = '#' . $element["time"]["#id"];
    $time2 = str_replace("value", "value2", $time1);

    kpr($element);
    drupal_add_js("Drupal.behaviors.date_autoset = function() {
        DateAutoset.register_dates($('" . $date1 . "'), $('" . $date2 . "'));
        DateAutoset.register_times($('" . $time1 . "'), $('" . $time2 . "'));
     };", "inline");
  }
  return $element;
}

function theme_date_autoset($element) {
  return $element['#children'];
}
